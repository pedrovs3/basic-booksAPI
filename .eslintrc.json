{
  "env": {
    "node": true,
    "es2022": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "class-methods-use-this": "off",
    "import/first": "off",
    "strict": "off",
    "no-param-reassign": "off",
    "camelcase": "off",
    "import/no-cycle": "off",
    "func-names": "off",
    "no-array-constructor": "off"
  }
}